{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://raw.githubusercontent.com/kmi-robots/json-schemas/master/geometry_msgs/Quaternion.schema.json",
  "title": "geometry_msgs/Quaternion",
  "description": "This represents an orientation in free space in quaternion form",
  "type": "object",
  "properties": {
    "x": {
      "type": "number"
    },
    "y": {
      "type": "number"
    },
    "z": {
      "type": "number"
    },
    "w": {
      "type": "number"
    }
  },
  "required": [ "x", "y", "z", "w" ]
}
